{
  "name": "fsh-sushi",
  "version": "0.6.2",
  "description": "Sushi Unshortens Short Hand Inputs (FSH Compiler)",
  "scripts": {
    "build": "rm -rf dist && tsc && cp -r src/ig/files dist/ig/files",
    "build:watch": "tsc -w",
    "build:grammar": "bash antlr/gradlew -p antlr generateGrammarSource",
    "test": "jest --coverage",
    "test:watch": "npm run test -- --watchAll",
    "coverage": "opener coverage/lcov-report/index.html",
    "lint": "tsc && eslint \"**/*.{js,ts}\"",
    "lint:fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier --check \"**/*.{js,ts}\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts}\"",
    "check": "npm run test && npm run lint && npm run prettier",
    "prepare": "npm run build",
    "prepublishOnly": "npm run check"
  },
  "contributors": [
    "Julia Afeltra <jafeltra@mitre.org>",
    "Nick Freiter <nfreiter@mitre.org>",
    "Dylan Mahalingam <kmahalingam@mitre.org>",
    "Chris Moesel <cmoesel@mitre.org>",
    "Mint Thompson <mathompson@mitre.org>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fhir/sushi.git"
  },
  "main": "dist/app.js",
  "bin": {
    "sushi": "dist/app.js"
  },
  "types": "dist/app.d.ts",
  "files": [
    "dist/**/*.{js,json,d.ts}",
    "dist/ig/files/**/*",
    "antlr/**/*.g4"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/antlr4": "^4.7.0",
    "@types/fs-extra": "^8.0.1",
    "@types/ini": "^1.3.30",
    "@types/jest": "^24.0.22",
    "@types/lodash": "^4.14.145",
    "@types/node": "^12.12.6",
    "@types/request-promise-native": "^1.0.17",
    "@types/sax": "^1.2.1",
    "@types/tar": "^4.0.3",
    "@types/temp": "^0.8.34",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "acorn": "^7.1.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "opener": "^1.5.1",
    "prettier": "1.19.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@types/html-minifier": "^3.5.3",
    "antlr4": "^4.7.2",
    "commander": "^4.0.1",
    "fs-extra": "^8.1.0",
    "html-minifier": "^4.0.0",
    "ini": "^1.3.5",
    "lodash": "^4.17.15",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "sax": "^1.2.4",
    "tar": "^5.0.5",
    "temp": "^0.9.1",
    "winston": "^3.2.1"
  }
}
